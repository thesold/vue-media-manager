{
    "author": "Marco Mark (m2de) <m2de@outlook.com>",
    "main": "dist/index.js",
    "description": "Stand-alone Vue.js Media Manager/Library",
    "name": "@thesold/vue-media-manager",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/thesold/vue-media-manager.git"
    },
    "scripts": {
        "build": "npm run rollup && npm run tailwind && npm run purge && npm version patch",
        "rollup": "rollup -c",
        "tailwind": "tailwind build ./src/assets/style.css -c ./tailwind.js -o ./dist/style.css",
        "purge": "purgecss --css ./dist/style.css --content ./src/components/MediaManager.vue --out ./dist/",
        "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
        "e2e": "node test/e2e/runner.js",
        "start": "npm run dev",
        "test": "npm run unit && npm run e2e",
        "unit": "jest --config test/unit/jest.conf.js --coverage",
        "precommit": "lint-staged"
    },
    "version": "1.0.2",
    "files": [
        "dist/*",
        "README.md"
    ],
    "browserslist": [
        "> 3%",
        "last 2 versions",
        "not ie <= 11"
    ],
